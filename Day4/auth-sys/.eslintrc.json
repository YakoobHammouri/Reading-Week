{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "consistent-return": "warn",
    "operator-linebreak": "off",
    "prettier/prettier": ["error"]
  }
}
